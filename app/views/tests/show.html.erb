<h1><%= @quiz.name %></h1>
<%= @test.name %>

<ol class="questions">
<% @quiz.questions.each do |question| %>
  <li>
    <%= question.title %>
    <%= question.explanation %>

    <ol class="answers">
      <% question.answers.each do |answer| %>
        <li><%= answer.content %></li>
      <% end %>
    </ol>
  </li>
<% end %>
</ol>


